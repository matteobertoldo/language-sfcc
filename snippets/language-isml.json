{
  ".text.html.isml": {
    "Active Data Context": {
      "prefix": "activedatacontext",
      "body": "<isactivedatacontext category=\"${${1:current_category}}\"/>$0"
    },
    "Active Data Head": {
      "prefix": "activedatahead",
      "body": "<isactivedatahead/>$0"
    },
    "Analytics Off": {
      "prefix": "analyticsoff",
      "body": "<isanalyticsoff/>$0"
    },
    "Break": {
      "prefix": "break",
      "body": "<isbreak/>$0"
    },
    "Cache": {
      "prefix": "cache",
      "body": "<iscache status=\"$1on\" type=\"relative|daily\" hour=\"duration_hr\" minute=\"duration_min\" varyby=\"price_promotion\" if=\"${boolean_expression}\"/>$0"
    },
    "Comment": {
      "prefix": "comment",
      "body": "<iscomment> $1 </iscomment>$0"
    },
    "Component": {
      "prefix": "component",
      "body": "<iscomponent pipeline=\"${1:pipeline_name}\" locale=\"locale_name\"/>$0"
    },
    "Content": {
      "prefix": "content",
      "body": "<iscontent type=\"${1:MIME_type}\" charset=\"char_set\" encoding=\"on|off|html|xml|wml\" compact=\"true|false\"/>$0"
    },
    "Continue": {
      "prefix": "continue",
      "body": "<iscontinue/>$0"
    },
    "Cookie": {
      "prefix": "cookie",
      "body": "<iscookie name=\"${1:cookie_name}\" value=\"cookie_value\" comment=\"cookie_use\" domain=\"domain_name\" path=\"pathname\" maxAge=\"cookie_lifetime\" version=\"cookie_spec\" secure=\"on|off\"/>$0"
    },
    "Decorate": {
      "prefix": "decorate",
      "body": "<isdecorate template=\"${1:template_name}\">\n\n</isdecorate>$0"
    },
    "Else Condition": {
      "prefix": "else",
      "body": "<iselse>$0"
    },
    "Else If Condition": {
      "prefix": "elseif",
      "body": "<iselseif condition=\"${${1:true}}\">\n\n</iselseif>$0"
    },
    "Else If Condition Inline": {
      "prefix": "elseif-inline",
      "body": "<iselseif condition=\"${${1:true}}\">$0"
    },
    "If Condition": {
      "prefix": "if",
      "body": "<isif condition=\"${${1:true}}\">\n\n</isif>$0"
    },
    "Ife Condition": {
      "prefix": "ife",
      "body": "<isif condition=\"${${1:true}}\">\n\n<iselse>\n\n</isif>$0"
    },
    "Include": {
      "prefix": "include",
      "body": "<isinclude template=\"${1:template_name}\"/>$0"
    },
    "Loop": {
      "prefix": "loop",
      "body": "<isloop items=\"${${1:item_object}}\" var=\"var_name\" status=\"var_name\" begin=\"${expression}\" end=\"${expression}\" step=\"${expression}\">\n\n</isloop>"
    },
    "Module": {
      "prefix": "module",
      "body": "<ismodule template=\"${${1:template_path}}\" name=\"tag_name\" attribute=\"attr_name\"/>$0"
    },
    "Next": {
      "prefix": "next",
      "body": "<isnext/>$0"
    },
    "Object ISML": {
      "prefix": "object-isml",
      "body": "<isobject object=\"${${1:name}}\" view=\"none|searchhit|recommendation|setproduct|detail\">\n\n</isobject>$0"
    },
    "Print": {
      "prefix": "print",
      "body": "<isprint value=\"${${1:output}}\" formatter=\"format\" encoding=\"on|off\" timezone=\"SITE|INSTANCE|utc\" padding=\"padding_constant\"/>$0"
    },
    "Redirect": {
      "prefix": "redirect",
      "body": "<isredirect location=\"${${1:target_url}}\" permanent=\"true|false\"/>"
    },
    "Remove": {
      "prefix": "remove",
      "body": "<isremove name=\"${1:var_name}\" scope=\"session|pdict|request|page\"/>"
    },
    "Replace": {
      "prefix": "replace",
      "body": "<isreplace/>$0"
    },
    "Script ISML": {
      "prefix": "script-isml",
      "body": "<isscript>$1\n\n</isscript>$0"
    },
    "Select ISML": {
      "prefix": "select-isml",
      "body": "<isselect name=\"${1:simple_name}\" iterator=\"${loop_var}\" description=\"description\" value=\"text_value\" condition=\"${true||false}\" encoding=\"on|off\"/>$0"
    },
    "Set": {
      "prefix": "set",
      "body": "<isset name=\"${1:var_name}\" value=\"${var_value}\" scope=\"session|request|page\"/>$0"
    },
    "Slot": {
      "prefix": "slot",
      "body": "<isslot id=\"${1:id}\" context=\"global|category\" context-object=\"${context_object}\" description=\"description\" preview-url=\"${url}\"/>$0"
    },
    "Status": {
      "prefix": "status",
      "body": "<isstatus value=\"${1:http_code}\"/>$0"
    }
  },
  ".text.html.isml .meta.tag, .text.html.isml .embedded": {
