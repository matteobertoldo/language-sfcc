      "prefix": "decorate"
    },
    "Else If Condition Inline": {
      "prefix": "elseif-inline"
    },
    "Include": {
      "prefix": "include"
    },
    "Loop": {
      "prefix": "loop"
    },
    "Module": {
      "prefix": "module"
    },
    "Next": {
      "prefix": "next"
    },
    "Object ISML": {
      "prefix": "object-isml"
    },
    "Print": {
      "prefix": "print"
    },
    "Redirect": {
      "prefix": "redirect"
    },
    "Remove": {
      "prefix": "remove"
    },
